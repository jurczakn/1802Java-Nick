function login(){
	
	var xhr = new XMLHttpRequest();
	
	xhr.onreadystatechange = function(){
		
		if(xhr.status === 200 && xhr.readystate === 4){
			
			var data = xhr.response;
			
			
			
		}
		
	}
	
}

window.onload = function(){
	
	document.getElementById("btn").addEventListener("click", login, false);
	
}