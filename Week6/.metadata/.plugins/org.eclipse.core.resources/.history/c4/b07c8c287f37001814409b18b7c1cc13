package com.revature.pojo;

public class Banana {
	
	private Peel bananaPeel;
	
	private String name;

	public Banana() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Banana(Peel peel, String name) {
		super();
		this.bananaPeel = peel;
		this.name = name;
	}

	public Peel getPeel() {
		return bananaPeel;
	}

	public void setPeel(Peel peel) {
		this.bananaPeel = peel;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "Banana [peel=" + bananaPeel + ", name=" + name + "]";
	}

}
